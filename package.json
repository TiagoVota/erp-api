{
  "name": "erp-api",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "start:dev": "npm run prisma:push && node ./src/server.js",
    "start:test": "dotenv -e .env.test npx nodemon ./src/server.js",
    "start:watch": "npx nodemon ./src/server.js",
    "test": "npm run prisma:push-test && NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test jest -- -i",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test jest -- --watch -i",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test npm run test -- -- --coverage",
    "git:update": "git add . && git status",
    "prisma:generate": "npx prisma generate",
    "prisma:view-db": "npx prisma studio",
    "prisma:format": "npx prisma format",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:push": "npx prisma db push",
    "prisma:push-test": "dotenv -e .env.test npm run prisma:push",
    "seed": "npm run prisma:push && node prisma/seed/seed.js",
    "seed:test": "dotenv -e .env.test npm run seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
		"@faker-js/faker": "^7.3.0",
    "@prisma/client": "^4.1.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
		"dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "string-strip-html": "^11.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.1",
    "supertest": "^6.2.4"
  }
}
